<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="LifeStyle">
	
	<class>com.LifeStyle.Entitymodels.User_Login</class>
	<class>com.LifeStyle.Entitymodels.UserProfile</class>
	<class>com.LifeStyle.Entitymodels.UserPersonal</class>
	<class>com.LifeStyle.Entitymodels.FoodList</class>
	<class>com.LifeStyle.Entitymodels.UserFoodIntake</class>
	<class>com.LifeStyle.Entitymodels.ExerciseList</class>
	<class>com.LifeStyle.Entitymodels.UserExercise</class>
	<properties>
		<property name="javax.persistence.jdbc.url" value="jdbc:mariadb://localhost:3306/lifestyle"/>
		<property name="javax.persistence.jdbc.driver" value="org.mariadb.jdbc.Driver"/>
		<property name="javax.persistence.jdbc.user" value="mydb"/>
		<property name="javax.persistence.jdbc.password" value=""/>
		<property name="eclipselink.ddl-generation" value="create-tables"/>
		<property name="eclipselink.target-database" value="MYSQL"/>
		<property name="eclipselink.logging.level" value="OFF"/>
	</properties>
	
	
	
	</persistence-unit>
</persistence>
